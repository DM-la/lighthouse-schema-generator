{
    "name": "dm/lighthouse-schema-generator",
    "keywords": ["laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "Dima Yefremov",
            "email": "efremov2293@gamil.com"
        },
        {
            "name": "Max Shcherbyna",
            "email": "ffwmaksshcherbyna@gmail.com"
        }
    ],
    "require": {
        "php": ">= 7.2",
        "nuwave/lighthouse": "^5.12",
        "doctrine/dbal": "^3.1"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "vimeo/psalm": "^4.12",
        "nunomaduro/larastan": "^1.0",
        "phpstan/phpstan": "^1.1"
    },
    "autoload": {
        "psr-4": { "DM\\LighthouseSchemaGenerator\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": {
            "DM\\LighthouseSchemaGenerator\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DM\\LighthouseSchemaGenerator\\SchemaGeneratorServiceProvider"
            ]
        }
    }
}
